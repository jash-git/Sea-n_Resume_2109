!function(o,e){var a=e.DFP||{};a.setcookie=function(e){o.ajax({url:"https://api.gamer.com.tw/buap/",method:"GET",dataType:"json",data:e}).done(function(o){void 0==o.code&&void 0==o.message&&(Cookies.set("buap_modr",o.buap_modr,{expires:14,domain:".gamer.com.tw"}),Cookies.set("buap_puoo",o.buap_puoo,{expires:14,domain:".gamer.com.tw"}))})},a.setTargeting=function(){var o=Cookies.get("buap_modr")||"";o&&googletag.pubads().setTargeting("modr",o);var e=Cookies.get("buap_puoo")||"";e&&googletag.pubads().setTargeting("puoo",e)},e.DFP=a}(jQuery,window);